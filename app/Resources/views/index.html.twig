<script type="text/javascript">
    ;(function($) {

        $('.swipebox').swipebox();

        $('#contact-form').on('submit', function(e) {
            var $form = $(this),
                data = {},
                inputs,
                errors,
                i;
            e.preventDefault();

            $form.find('input, textarea').each(function(i, el) {
                data[$(el).attr('name')] = $(el).val();
            });

            $.ajax({
                data: data,
                method: 'post',
                url: '{{ path('ajax_contact') }}',
                success: function(r) {
                    $form
                        .find('input, textarea')
                        .css('background-color', '#627961')
                        .closest('form')
                        .find('button')
                        .attr('disabled', 'disabled')
                        .css({
                            'pointer-events': 'none',
                            'cursor': 'not-allowed',
                            'background-color': '#ccc'
                        });
                },
                error: function(r) {
                    inputs = Object.keys(data),
                    errors = r.responseJSON;
                    for (i = 0; i < inputs.length; i++) {
                        if (errors[inputs[i]] == 'empty') {
                            $form.find('#' + inputs[i]).css('background-color', '#B54A4A');
                        } else if (!(inputs[i] in errors)) {
                            $form.find('#' + inputs[i]).css('background-color', '#2C2A2A');
                        }
                    }
                }
            });
        });

    })(jQuery);
</script>



